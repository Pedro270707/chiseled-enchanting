  "authors": [
    "Pedro Ricardo"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/chiseledenchanting/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.pedroricardo.chiseledenchanting.ChiseledEnchanting"
    ]
  },
  "mixins": [
    "chiseledenchanting.mixins.json",
    {
      "config": "chiseledenchanting.client.mixins.json",
      "environment": "client"
    }
